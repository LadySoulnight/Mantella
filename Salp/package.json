{
  "name": "@mantella/salp",
  "version": "0.5.0",
  "license": "MIT",
  "devDependencies": {
    "eslint": "5.16.*",
    "eslint-plugin-jest": "22.5.*",
    "eslint-plugin-import": "2.19.*",
    "jest": "24.8.*",
    "rollup": "1.27.*"
  },
  "scripts": {
    "build": "yarn build:cjs && yarn build:esm",
    "build:cjs": "rollup ./src/index.js --format cjs --file ./dist/salp.cjs.js",
