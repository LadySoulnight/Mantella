{% 
for _interface in site.data.api %}{%
  assign _url = _interface.title | slugify %}{%
  for _group in _interface.groups %}{%
    for _function in _group.functions %}{%
      capture _signature %}{{ _url }}-{% include function call=_function type="signature" %}{% endcapture %}{%
      if include.signature == _signature %}{% include function call=_function prefix=_url type="short" %}{%
      endif %}{%
    endfor %}{%
  endfor %}{%

  assign _url_prefix = _url %}{%
  for _interface in _interface.childs %}{% 
    capture _url = _interface.title %}{{ _url_prefix }}/{{ _interface.title | slugify }}{% endcapture %}{%
    for _group in _interface.groups %}{% 
      for _function in _group.functions %}{%
        capture _signature %}{{ _interface.title | slugify }}-{% include function call=_function type="signature" %}{% endcapture %}{%
        if include.signature == _signature %}{% include function call=_function prefix=_url type="short" %}{%
        endif %}{%
      endfor %}{%
    endfor %}{%
  endfor %}{%
endfor %}